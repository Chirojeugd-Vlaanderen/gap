<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
  </configSections>
  <userSettings>
    <unity>
      <containers>
        <container name="container">
          <types>
            <!-- map standaard naar dummy's die niets implementeren -->
            <type type="Chiro.Gap.Orm.DataInterfaces.IKindDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyKindDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.ILeidingDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyLeidingDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.IGroepenDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyGroepenDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.IPersonenDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyPersonenDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.IGelieerdePersonenDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyGelieerdePersonenDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.IAfdelingsJarenDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyAfdelingsJarenDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Gap.Orm.DataInterfaces.ICategorieenDao, Chiro.Gap.Orm"
                  mapTo="Chiro.Gap.Dummies.DummyCategorieenDao, Chiro.Gap.Dummies" />
            <type type="Chiro.Cdf.Data.IDao`1, Chiro.Cdf.Data"
                  mapTo="Chiro.Gap.Data.Ef.AbstractChiroGroepDao`1, Chiro.Gap.Data" />
            <!-- autorisatiemanager vindt altijd alles ok -->
            <type type="Chiro.Gap.Workers.IAutorisatieManager, Chiro.Gap.Workers"
                  mapTo="Chiro.Gap.Dummies.AutMgrAltijdGav, Chiro.Gap.Dummies" />
          </types>
        </container>
      </containers>
    </unity>
  </userSettings>
</configuration>
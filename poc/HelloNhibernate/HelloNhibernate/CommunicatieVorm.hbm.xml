<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" 
                   schema="pers" assembly="HelloNhibernate"
                   namespace="HelloNhibernate">
  <class name="CommunicatieVorm" lazy="false">
    <id name="ID" access="nosetter.lowercase-underscore" column="CommunicatieVormID">
      <generator class="native" />
    </id>
    <property name="Nummer" access="property" column="Nummer" not-null="true" />
    <property name="TypeInt" access="property" column="Type" not-null="true" />
    <property name="Voorkeur" access="property" column="Voorkeur" not-null="true" />
    <property name="Versie" column="Versie" insert="false" update="false" />
    <many-to-one name="Persoon" access="property" column="PersoonID" cascade="all" not-null="true" />
  </class>
</hibernate-mapping>
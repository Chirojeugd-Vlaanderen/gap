<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
                   schema="pers" assembly="Cg2.Core"
                   namespace="Cg2.Core.Domain">
  <class name="CommunicatieVorm" lazy="false">
    <id name="ID" column="CommunicatieVormID">
      <generator class="native" />
    </id>
    <property name="TypeInt" column="CommunicatieTypeID" not-null="true" />
    <property name="Nummer" column="Nummer" not-null="true" />
    <property name="IsGezinsGebonden" column="IsGezinsGebonden" not-null="true" />
    <property name="Voorkeur" column="Voorkeur" not-null="true" />
    <property name="Nota" column="Nota" />
    <property name="Versie" column="Versie" insert="false" update="false" />
    <many-to-one name="Persoon" column="PersoonID" class="Persoon" not-null="true" />
    <!-- <property name="BusinessKey" column="Guid" /> -->
  </class>
</hibernate-mapping>